        <LocalizedStrings>
			
			<!-- ================================================== Normaux ================================================== -->
			
			<GameDBLocalizedString>	<LocID>TRT_IMMUNOGLOBULINE_ANTI-D</LocID>							<Text>Immunoglobulin infusions</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_IMMUNOGLOBULINE_ANTI-D_DESCRIPTION</LocID>				<Text>A sterile solution of concentrated antibodies extracted from healthy people that is administered directly into a vein..</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_ANTI_ACNEE</LocID>										<Text>Anti-acneic</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_ANTI_ACNEE_DESCRIPTION</LocID>							<Text>Medicine used to treat various types and forms of acne.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OP</LocID>												<Text>Estradiol-Progesterone Capsule</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OP_DESCRIPTION</LocID>									<Text>This medication contains 2 female hormones: an estrogen and a progestin.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_METFORMINE</LocID>										<Text>Anti-diabetic drug</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_METFORMINE_DESCRIPTION</LocID>							<Text>Medications that reduce insulin resistance.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_ANTI_ANDROGEN</LocID>									<Text>Anti-androgens</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_ANTI_ANDROGEN_DESCRIPTION</LocID>						<Text>Antiandrogen agents block androgen receptors, thereby inhibiting the effects of male sex hormones.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_PROGESTATIF</LocID>										<Text>Progestogen</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_PROGESTATIF_DESCRIPTION</LocID>							<Text>A naturally occurring or synthetic progestational steroid.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OESTROGENOTHERAPIE</LocID>								<Text>Estrogen Replacement Therapy</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OESTROGENOTHERAPIE_DESCRIPTION</LocID>					<Text>Hormone replacement therapy involving the administration of estrogen without progestin.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_THM</LocID>												<Text>HRT</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_THM_DESCRIPTION</LocID>									<Text>Postmenopausal hormone therapy (HRT).</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_THERAPIE_HORMONALE</LocID>								<Text>Hormone therapy</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_THERAPIE_HORMONALE_DESCRIPTION</LocID>					<Text>The therapeutic use of hormones.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_PESSAIRE</LocID>											<Text>Pessary</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_PESSAIRE_DESCRIPTION</LocID>								<Text>A device worn in the vagina to support the uterus, remedy a malposition, or prevent conception.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TOCOLYTIQUE</LocID>										<Text>Tocolytic</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TOCOLYTIQUE_DESCRIPTION</LocID>							<Text>Inhibiting uterine contractions.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_MYOLYSE</LocID>											<Text>High-Intensity Focused Ultrasound</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_MYOLYSE_DESCRIPTION</LocID>								<Text>High-intensity focused ultrasound (HIFU) is a highly precise medical procedure used locally to heat and destroy diseased tissue through ablation.</Text>	</GameDBLocalizedString>

			<GameDBLocalizedString>	<LocID>TRT_SURVEILLANCE</LocID>										<Text>Medical care</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_SURVEILLANCE_DESCRIPTION</LocID>							<Text>The provision of what is necessary for a person's health and well-being by a doctor, nurse, or other healthcare professional.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OCYTOCINE</LocID>										<Text>Oxytocin</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OCYTOCINE_DESCRIPTION</LocID>							<Text>A hormone released by the pituitary gland that causes increased contraction of the uterus during labour and stimulates the ejection of milk into the ducts of the breasts.</Text>	</GameDBLocalizedString>

			<GameDBLocalizedString>	<LocID>TRT_ASPIRATION_ENDO_UTERINE</LocID>							<Text>Vacuum aspiration</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_ASPIRATION_ENDO_UTERINE_DESCRIPTION</LocID>				<Text>Vacuum or suction aspiration uses aspiration to remove uterine contents through the cervix.</Text>	</GameDBLocalizedString>

			<GameDBLocalizedString>	<LocID>TRT_OBGYN_REEDUCATION_PELV</LocID>							<Text>Perineo-sphincter rehabilitation</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OBGYN_REEDUCATION_PELV_DESCRIPTION</LocID>				<Text>Intravaginal manual work of the pelvic floor muscles, improves the control of urination.</Text>	</GameDBLocalizedString>


			<!-- ================================================== Chirurgie ================================================== -->

			<GameDBLocalizedString>	<LocID>TRT_SALPINGOTOMIE</LocID>									<Text>Salpingotomy</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_SALPINGOTOMIE_DESCRIPTION</LocID>						<Text>The operation of making an incision into a fallopian tube to remove an ectopic.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_DETORSION</LocID>										<Text>Detorsion and cystectomy</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_DETORSION_DESCRIPTION</LocID>							<Text>Surgical detorsion of adnexa.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_KYSTECTOMIE</LocID>										<Text>Cystectomy</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_KYSTECTOMIE_DESCRIPTION</LocID>							<Text>The surgical excision of a cyst.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_DRAINAGE</LocID>											<Text>Drainage</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_DRAINAGE_DESCRIPTION</LocID>								<Text>Surgical drainage.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TOILETTE_PERINEAL</LocID>								<Text>Peritoneal cleansing</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TOILETTE_PERINEAL_DESCRIPTION</LocID>					<Text>Surgical peritoneal cleansing.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_CESARIENNE</LocID>										<Text>Cesarean section</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_CESARIENNE_DESCRIPTION</LocID>							<Text>A surgical procedure involving incision of the walls of the abdomen and uterus for delivery of offspring.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OBGYN_ENDOSCOPIC_CAUTERIZATION</LocID>						<Text>Endoscopic cauterization</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OBGYN_ENDOSCOPIC_CAUTERIZATION_DESCRIPTION</LocID>			<Text>Endoscopic cauterization is a surgical procedure where an electric current is used to burn/cautherize an area in order to open or close it off. It is commonly used to stop bleedings during surgeries.</Text>	</GameDBLocalizedString>

			<GameDBLocalizedString>	<LocID>TRT_EXERESE</LocID>											<Text>Surgical resection.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_EXERESE_DESCRIPTION</LocID>								<Text>The surgical removal of part of an organ or structure..</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_HYSTERECTOMIE</LocID>									<Text>Hysterectomy</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_HYSTERECTOMIE_DESCRIPTION</LocID>						<Text>A surgical operation to remove all or part of the uterus.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TUMORECTOMIE</LocID>										<Text>Lumpectomy</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TUMORECTOMIE_DESCRIPTION</LocID>							<Text>An operation to remove the cancer and some normal tissue around it, but not the breast itself.</Text>	</GameDBLocalizedString>

