			<GameDBLocalizedString>	<LocID>TRT_TUMORECTOMIE</LocID>										<Text>Lumpectomía</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TUMORECTOMIE_DESCRIPTION</LocID>							<Text>Una operación para extirpar el cáncer y parte del tejido normal que lo rodea, pero no el seno en sí.</Text>	</GameDBLocalizedString>
		
	
