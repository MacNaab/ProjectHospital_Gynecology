			<GameDBLocalizedString>	<LocID>TRT_TOILETTE_PERINEAL</LocID>								<Text>腹膜清潔</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TOILETTE_PERINEAL_DESCRIPTION</LocID>					<Text>清潔腹膜的手術.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_CESARIENNE</LocID>										<Text>剖腹產</Text>	</GameDBLocalizedString>
