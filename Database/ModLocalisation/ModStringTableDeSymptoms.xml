			<GameDBLocalizedString>	<LocID>SYM_AMENORRHEE_DESCRIPTION</LocID>			<Text>Abnormale Abwesenheit oder Unterdrückung der Menstruation.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>SYM_MASTODYNIE</LocID>						<Text>Mastodynia </Text>	</GameDBLocalizedString>
