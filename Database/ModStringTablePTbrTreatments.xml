			<GameDBLocalizedString>	<LocID>TRT_PESSAIRE</LocID>											<Text>Pessário</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_PESSAIRE_DESCRIPTION</LocID>								<Text>Um dispositivo usado na vagina para apoiar o útero, remediar uma posição incorreta ou impedir a concepção.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_TOCOLYTIQUE</LocID>										<Text>Tocolítico</Text>	</GameDBLocalizedString>
