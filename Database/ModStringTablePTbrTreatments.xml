			<GameDBLocalizedString>	<LocID>TRT_ANTI_ACNEE</LocID>										<Text>Anti-acneico</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_ANTI_ACNEE_DESCRIPTION</LocID>							<Text>Medicamento usado para tratar vários tipos e formas de acne.</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OP</LocID>												<Text>Cápsula de estradiol-progesterona</Text>	</GameDBLocalizedString>
			<GameDBLocalizedString>	<LocID>TRT_OP_DESCRIPTION</LocID>									<Text>Este medicamento contém 2 hormônios femininos: estrogênio e progesterona.</Text>	</GameDBLocalizedString>
